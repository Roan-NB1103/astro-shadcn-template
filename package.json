{
  "name": "astro-shadcn-template",
  "type": "module",
  "version": "0.0.1",
  "engines": {
    "node": ">=20.9.0 <21.0.0",
    "npm": ">=10.1.0"
  },
  "volta": {
    "node": "20.18.0",
    "npm": "10.8.2"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development astro dev --mode development",
    "start": "astro dev",
    "build": "astro build && npm run format-html",
    "format-html": "prettier --write \"dist/**/*.html\"",
    "preview": "astro preview",
    "astro": "astro",
    "stg": "cross-env NODE_ENV=staging astro build --mode staging",
    "prod": "cross-env NODE_ENV=production astro build --mode production"
  },
  "dependencies": {
    "@astrojs/react": "^4.2.5",
    "@radix-ui/react-dialog": "^1.1.11",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-radio-group": "^1.3.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.2.2",
    "@tailwindcss/vite": "^4.1.4",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "astro": "^5.7.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.503.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sass-embedded": "^1.87.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.4"
  },
  "devDependencies": {
    "@sentry/node": "^9.14.0",
